<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nacatton.dicehabits" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".DiceHabitsActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SetHabitsActivity"
            android:label="@string/title_activity_set_habits" >
        </activity>
        <activity
            android:name=".GenerateHabitActivity"
            android:label="@string/title_activity_generate_habit" >
        </activity>
        <activity
            android:name=".JournalActivity"
            android:label="@string/title_activity_journey" >
        </activity>
    </application>

</manifest>
